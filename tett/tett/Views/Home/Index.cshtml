@model IEnumerable<tett.Models.Xe>
@using tett.Models

 <h2>@Session["TenUser"]</h2>


<a href="/Account/Login">Dang Nhap</a>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.TenXe)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MoTa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HinhAnh)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LoaiXe.TenLoaiXe)
        </th>
        <th></th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenXe)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MoTa)
        </td>
        <td>
            <img src="@Html.DisplayFor(modelItem => item.HinhAnh)" style="width:100px; height:100px"/>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LoaiXe.TenLoaiXe)
        </td>
        <td>
            <button type="submit" >Đặt</button>
        </td>
    </tr>
}

</table>
